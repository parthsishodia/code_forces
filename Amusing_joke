import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner Sc =new Scanner(System.in);
	   String name1=Sc.next().toLowerCase();
	   String name2=Sc.next().toLowerCase();
	   String result=name1+name2;
	   String name3=Sc.next().toLowerCase();
	   char[] res=result.toCharArray();
	   Arrays.sort(res);
	    String sortedResult=String.valueOf(res);
	    char[] thirdword=name3.toCharArray();
	    Arrays.sort(thirdword);
	    String word=String.valueOf(thirdword);
	    
	    
	   if(word.equals(sortedResult))
	   {
	       System.out.print("YES");
	   }
	   else{
	       System.out.print("NO");
	   }
}
}
