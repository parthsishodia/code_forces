import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner Sc =new Scanner(System.in);
		int cases=Sc.nextInt();
		int count=0;
		for(int i=0;i<cases;i++)
		{
		    int a=Sc.nextInt();
		    int b=Sc.nextInt();
		    int c=Sc.nextInt();
		    int d=Sc.nextInt();
		   if(a<b)
		   count++;
		   if(a<c)
		   count++;
		   if(a<d)
		   count++;
		   System.out.println(count);
		   
		   count=0;
		}
		
	}
}
