import java.util.Scanner;
 public class Elephant
{
    public static void main(String args[])
    {
        Scanner Sc =new Scanner (System.in);
        int x= Sc.nextInt();
       int count =0;
       
        if(x%5!=0)
        {
            count++;
        }
        int step = count + x/5;
        System.out.print(step);
    }
}
