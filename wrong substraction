import java.util.Scanner;
public class wrong_subs {
    public static void main(String args[])
    {   
        Scanner Sc= new Scanner(System.in);
        int n=Sc.nextInt();
        int i;
        int k=Sc.nextInt();
        for( i=0;i<k;i++)
        {
            if(n%10==0)
            {
                n=n/10;
            }
            else
            {
                n=n-1;
            }
        }
        System.out.print(n);
    }}
