import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner Sc=new Scanner(System.in);
		String S1=Sc.next();
		String res="";
		 S1=S1.toLowerCase();
	   res=S1.replaceAll("[aeiouy]","");
	    char word[]=res.toCharArray();
		for(int i=0;i<word.length;i++)
		{
		    System.out.print("."+word[i]);
		}
	}
}
