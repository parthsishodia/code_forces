import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner Sc =new Scanner(System.in);
	    int  n=Sc.nextInt();
	   while(n--!=0)
	    {    int a=Sc.nextInt();
	    int b=Sc.nextInt();
	    if(a%b==0)
	    {
	        System.out.println(0);
	    }
	    else{
	     System.out.println(b-(a%b));
	    }
	    
	    }
	}
}
