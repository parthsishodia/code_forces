
import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner Sc=new Scanner(System.in);
		int n=Sc.nextInt();
		int count=0;
		for(int i=0;i<n;i++)
		{
		    int ans=Sc.nextInt();
		    if(ans==1)
		    {
		        count = 1;
		      // System.out.println("Hard");
		    }
		    
		    }
		    if(count==1)
		    {
		        System.out.print("Hard");
		        
		    }
		    else
		    {
		        System.out.print("Easy");
		    }
		    
		
	
		
	
	}
}
